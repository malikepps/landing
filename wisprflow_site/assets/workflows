<!--Resize Input Horizontally-->
<script>
  document.addEventListener('DOMContentLoaded', function() {
    const elements = document.querySelectorAll('[ms-code-resize-input="width"]');

    // Store the initial widths
    const initialWidths = new Map();
    elements.forEach(element => {
        initialWidths.set(element, element.offsetWidth);
    });

    elements.forEach(element => {
        element.addEventListener('input', function() {
            autoResizeWidth(this);
        });
    });

    function autoResizeWidth(element) {
        // Find the nearest hidden measure element
        const measurer = element.nextElementSibling.getAttribute('ms-code-resize-input') === 'hidden-measure' 
                         ? element.nextElementSibling 
                         : null;
        if (!measurer) return; // Exit if no measurer is found

        measurer.textContent = element.value;

        const maxWidth = parseInt(getComputedStyle(element).maxWidth, 10);
        const minWidth = initialWidths.get(element);
        const contentWidth = measurer.offsetWidth;

        if (contentWidth > minWidth && contentWidth < maxWidth) {
            element.style.width = `${contentWidth}px`;
        } else if (contentWidth >= maxWidth) {
            element.style.width = `${maxWidth}px`;
        } else {
            element.style.width = `${minWidth}px`;
        }
    }
  });
</script>

<!--Combo box clear button function-->
<script>
document.addEventListener('DOMContentLoaded', function() {
    const clearButtons = document.querySelectorAll('[fs-combobox-element="clear"]');
    
    clearButtons.forEach(button => {
        button.addEventListener('click', function(event) {
            const dropdownToggle = this.closest('.w-dropdown-toggle');
            
            // Immediately close dropdown before anything else
            if (dropdownToggle) {
                dropdownToggle.classList.remove('w--open');
                const dropdownList = document.querySelector('#' + dropdownToggle.getAttribute('aria-controls'));
                if (dropdownList) {
                    dropdownList.classList.remove('w--open');
                }
                dropdownToggle.setAttribute('aria-expanded', 'false');
            }
            
            // Handle input separately
            const input = dropdownToggle.querySelector('input[fs-combobox-element="text-input"]');
            if (input) {
                input.disabled = true;
                input.blur();
                
                // Re-enable and remove focus after a delay
                setTimeout(() => {
                    input.disabled = false;
                    dropdownToggle.dispatchEvent(new Event('mouseup'));
                    dropdownToggle.dispatchEvent(new Event('mouseleave'));
                    
                    // Remove focus after everything else
                    setTimeout(() => {
                        input.blur();
                        document.body.focus();
                        const clickEvent = new MouseEvent('mousedown', {
                            bubbles: true,
                            cancelable: true,
                            view: window
                        });
                        document.body.dispatchEvent(clickEvent);
                    }, 50);
                }, 1);
            }
        });
    });
});
</script>

<!--Set logo of tool in combo box-->
<script>
document.addEventListener('DOMContentLoaded', function() {
    // Get elements
    const currentToolLogo = document.querySelector('[data-element="current-tool-logo"]');
    const toolLogos = document.querySelectorAll('[data-element="tool-logo"]');
    const filterInput = document.querySelector('[fs-cmsfilter-field="tool"]');

    // Hide current logo immediately
    if (currentToolLogo) {
        currentToolLogo.style.display = 'none';
    }

    // Create logo map
    const logoMap = new Map();
    
    toolLogos.forEach(function(logo) {
        const name = logo.getAttribute('logo-name');
        const src = logo.getAttribute('src');
        if (name && src) {
            logoMap.set(name.toLowerCase(), src);
        }
    });

    // Handle input changes
    if (filterInput) {
        filterInput.addEventListener('input', function(e) {
            const searchText = e.target.value.toLowerCase().trim();
            
            if (currentToolLogo) {
                if (logoMap.has(searchText)) {
                    currentToolLogo.src = logoMap.get(searchText);
                    currentToolLogo.style.display = 'block';
                } else {
                    currentToolLogo.style.display = 'none';
                }
            }
        });
    }
});
</script>

<!--Adding logos in tool dropdown-->
<script>
document.addEventListener('DOMContentLoaded', function() {
    function syncLogos() {
        // Create a map of tool names to their logo URLs
        const logoMap = new Map();
        const toolNames = document.querySelectorAll('[data-element="tool-name"]');
        const toolLogos = document.querySelectorAll('[data-element="tool-logo"]');
        
        // Build the map using tool names as keys
        toolNames.forEach((nameElement, index) => {
            const name = nameElement.textContent.trim();
            const logo = toolLogos[index];
            if (name && logo) {
                logoMap.set(name, logo.getAttribute('src'));
            }
        });

        // Update dropdown logos based on their corresponding names
        const dropdownNames = document.querySelectorAll('[data-element="dropdown-tool-name"]');
        const dropdownLogos = document.querySelectorAll('[data-element="dropdown-tool-logo"]');
        
        dropdownNames.forEach((nameElement, index) => {
            const name = nameElement.textContent.trim();
            const logoUrl = logoMap.get(name);
            const dropdownLogo = dropdownLogos[index];
            
            if (logoUrl && dropdownLogo) {
                dropdownLogo.setAttribute('src', logoUrl);
            }
        });
    }

    // Initial sync with retries
    setTimeout(() => {
        if (!syncLogos()) {
            let attempts = 0;
            const interval = setInterval(() => {
                attempts++;
                if (syncLogos() || attempts >= 5) {
                    clearInterval(interval);
                }
            }, 50);
        }
    }, 250);

    // Handle input changes
    const filterInput = document.querySelector('[fs-cmsfilter-field="tool"]');
    if (filterInput) {
        // Regular input/change events
        ['input', 'change'].forEach(event => {
            filterInput.addEventListener(event, () => setTimeout(syncLogos, 250));
        });

        // Handle modifier keys
        filterInput.addEventListener('keydown', (e) => {
            // Immediately sync on any modifier key or special key press
            if (e.metaKey || e.ctrlKey || e.altKey || e.shiftKey || 
                e.key === 'Backspace' || e.key === 'Delete' || 
                e.key === 'ArrowUp' || e.key === 'ArrowDown' || 
                e.key === 'Enter' || e.key === 'Escape') {
                syncLogos();
                // Schedule another sync to catch any delayed updates
                setTimeout(syncLogos, 250);
            }
        });

        // Handle modifier key release
        filterInput.addEventListener('keyup', (e) => {
            if (e.key === 'Meta' || e.key === 'Control' || 
                e.key === 'Alt' || e.key === 'Shift' || 
                e.key === 'Backspace' || e.key === 'Delete') {
                setTimeout(syncLogos, 250);
            }
        });
    }

    // Handle dropdown toggles
    document.querySelectorAll('.w-dropdown-toggle').forEach(toggle => {
        toggle.addEventListener('click', () => setTimeout(syncLogos, 250));
    });
});
</script>

<!--Posthog events for filter and card clicks-->
<script>
document.addEventListener('DOMContentLoaded', () => {
    function handleFilterClick(filterDropdown) {
        const optionTemplate = filterDropdown.querySelector('[fs-combobox-element="option-template"]');
        if (optionTemplate) {
            posthog.capture("workflow_filter_select", {
                "filter_type": optionTemplate.textContent.trim(),
                "page": "workflow_gallery"
            });
        }
    }

    function handleWorkflowClick(workflowCard) {
        const workflowTitle = workflowCard.querySelector('[workflow-name]');
        const workflowTools = Array.from(workflowCard.querySelectorAll('[workflow-card="tool-name"]'))
            .map(tool => tool.textContent.trim());
        const contributorElement = workflowCard.querySelector('[workflow-card="contributor-name"]');
        const cardLink = workflowCard.querySelector('[workflow-card-link]');

        const eventProperties = {
            "workflow_name": workflowTitle ? workflowTitle.textContent.trim() : 'Unknown Workflow',
            "workflow_tools": workflowTools,
            "page": "workflow_gallery",
            "total_tools": workflowTools.length,
            "contributor_name": contributorElement 
                ? contributorElement.textContent.trim() 
                : 'Unknown Contributor',
            "workflow_url": cardLink ? cardLink.getAttribute('href') : 'Unknown URL'
        };

        posthog.capture("workflow_card_click", eventProperties);
    }

    function attachListeners() {
        document.querySelectorAll('[filter-dropdown]').forEach(filterDropdown => {
            filterDropdown.onclick = () => handleFilterClick(filterDropdown);
        });

        document.querySelectorAll('[workflow-card-link]').forEach(cardLink => {
            cardLink.onclick = () => {
                const workflowCard = cardLink.closest('[workflow-card]');
                if (workflowCard) {
                    handleWorkflowClick(workflowCard);
                }
            };
        });
    }

    const observer = new MutationObserver(attachListeners);
    observer.observe(document.body, {
        childList: true,
        subtree: true
    });

    attachListeners();
});
</script>

<!--Posthog events for search query-->
<script>
document.addEventListener('DOMContentLoaded', () => {
  const searchInput = document.querySelector('[data-element="search-input"]');
  const searchSubmit = document.querySelector('[data-element="search-submit"]');
  const realSearchInput = document.querySelector('[data-element="real-search-input"]');
  
  // Function to handle search submission
  function handleSearchSubmit(e) {
    e.preventDefault();
    
    // Set the value
    realSearchInput.value = searchInput.value;
    
    // Track search query in PostHog if there's text
    const searchQuery = searchInput.value.trim();
    if (searchQuery && typeof posthog !== 'undefined') {
      posthog.capture("workflow_search_query", {
        "search_query": searchQuery,
        "page": "workflow_gallery"
      });
    }
    
    // Trigger input event to enable filtering
    const inputEvent = new Event('input', {
      bubbles: true,
      cancelable: true
    });
    realSearchInput.dispatchEvent(inputEvent);
  }
  
  // Ensure all elements exist
  if (searchInput && searchSubmit && realSearchInput) {
    // Watch for the expanded class
    const observer = new MutationObserver((mutations) => {
      mutations.forEach((mutation) => {
        if (mutation.type === 'attributes' && mutation.attributeName === 'class') {
          searchSubmit.style.display = searchInput.classList.contains('expanded') 
            ? 'block' 
            : 'none';
        }
      });
    });
    
    // Start observing the searchInput for class changes
    observer.observe(searchInput, { attributes: true });
    
    // Add click event listener to submit button
    searchSubmit.addEventListener('click', handleSearchSubmit);
    
    // Add keydown event listener to search input for Enter key
    searchInput.addEventListener('keydown', (e) => {
      if (e.key === 'Enter') {
        handleSearchSubmit(e);
      }
    });
  }
});
</script>

<!--Card byline-->
<script>
// Select all elements with the workflow-card="contributor-name" attribute
const contributorNames = document.querySelectorAll('[workflow-card="contributor-name"]');
contributorNames.forEach(element => {
    const fullName = element.textContent.trim();
    
    const nameParts = fullName.split(' ');
    
    // If we have at least a first and last name
    if (nameParts.length >= 2) {
        const firstName = nameParts[0];
        const lastInitial = nameParts[nameParts.length - 1][0];
        
        // Format the new name
        const formattedName = `${firstName} ${lastInitial}.`;
        
        // Update the element's text
        element.textContent = formattedName;
    }
});
</script></body></html>