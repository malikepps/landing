website-files.com/gsap/3.13.0/MotionPathHelper.min.js" type="text/javascript"></script><script src="https://cdn.prod.website-files.com/gsap/3.13.0/ScrollToPlugin.min.js" type="text/javascript"></script><script type="text/javascript">gsap.registerPlugin(MotionPathPlugin,SplitText,DrawSVGPlugin,ScrollTrigger,MotionPathHelper,ScrollToPlugin);</script><!--Navbar scroll animation-->
<script>
gsap.registerPlugin(ScrollTrigger);

const navbar = document.querySelector('.nav_fixed');

if (navbar && window.matchMedia('(max-width: 991px)').matches) {
  // Create the hide animation (from visible to hidden)
  const hideAnim = gsap.fromTo(navbar, 
    { yPercent: 0 }, 
    { yPercent: -100, duration: 0.5, ease: "power2.inOut", paused: true }
  );
  
  let lastScrollY = 0;
  let ticking = false;
  
  ScrollTrigger.create({
    start: "top top",
    end: "max",
    onUpdate: (self) => {
      const currentScrollY = self.scroll();
      if (!ticking) {
        requestAnimationFrame(() => {
          if (currentScrollY > lastScrollY && currentScrollY > 50) {
            // Scrolling down - hide navbar
            hideAnim.play();
          } else if (currentScrollY < lastScrollY) {
            // Scrolling up - show navbar
            hideAnim.reverse();
          }
          lastScrollY = currentScrollY;
          ticking = false;
        });
        ticking = true;
      }
    }
  });
}
</script>

<!-- Bottom Sticky Nav -->
<script>
if (window.matchMedia("(max-width: 991px)").matches) {
  window.addEventListener("load", () => {
    gsap.registerPlugin(ScrollTrigger);
    const bottomBar = document.querySelector(".nav_bottom");

    if (bottomBar && window.matchMedia("(max-width: 991px)").matches) {
      const showAnim = gsap.fromTo(
        bottomBar,
        { y: "100%" },
        {
          y: 0,
          duration: 0.5,
          ease: "power2.inOut",
          paused: true
        }
      );

      let lastScrollY = 0;
      let ticking = false;

      ScrollTrigger.create({
        start: "top top",
        end: "max",
        onUpdate: (self) => {
          const currentScrollY = self.scroll();
          if (!ticking) {
            requestAnimationFrame(() => {
              if (currentScrollY > lastScrollY && currentScrollY > 50) {
                showAnim.play();
              } else if (currentScrollY < lastScrollY) {
                showAnim.reverse();
              }
              lastScrollY = currentScrollY;
              ticking = false;
            });
            ticking = true;
          }
        }
      });
    }
  });
}
</script>

