  tracking.set('project_id','eed39acc-d24c-4129-a771-db2bcea87cf8');

  for (var i = 0; i < utmParams.length; i++) {
    var value = params.get(utmParams[i]);
    if (value) tracking.set(utmParams[i], value);
  }

  if (document.referrer) tracking.set('page_referrer', document.referrer);

  var pixelUrl =
    'https://yqagkqabd5.us-east-2.awsapprunner.com/api/track/pixel.gif?' +
    tracking.toString();

  var img = new Image();
