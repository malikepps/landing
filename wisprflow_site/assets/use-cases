<!-- bundle all your scripts into one -->
<script>
  gsap.registerPlugin(MotionPathPlugin);

  let iconTimelines = [];

  function initIconPath(desiredCount = 20) {
    iconTimelines = [];
    
    document.querySelectorAll('.use-case_svg-wrapper').forEach((wrapper, wrapperIndex) => {
      const pathEl = wrapper.querySelector('path');
      if (!pathEl) {
        return;
      }
      let icons = wrapper.querySelectorAll('.icon-item');
      if (icons.length === 0) {
        return;
      }
      const originals = Array.from(icons);
      while (icons.length < desiredCount) {
        const clone = originals[icons.length % originals.length].cloneNode(true);
        wrapper.appendChild(clone);
        icons = wrapper.querySelectorAll('.icon-item');
      }
      const tl = gsap.timeline({ repeat: -1 });
      icons.forEach((icon, i) => {
        const start = i / icons.length;
        tl.to(
          icon,
          {
            motionPath: {
              path: pathEl,
              align: pathEl,
              alignOrigin: [0.5, 0.5],
              autoRotate: true,
              start: start,
              end: start + 1,
            },
            ease: 'none',
            duration: 60,
          },
          0
        );
      });
      iconTimelines.push(tl);
    });
  }

  // 1) Run icon animation immediately
  initIconPath(52);
  
    window.addEventListener("resize", () => {
  if (window.innerWidth > 991) {
    initIconPath();
  }
});

  // 2) Once Finsweet’s ‘list’ Attribute is ready, restart each instance
  window.FinsweetAttributes ||= [];
  window.FinsweetAttributes.push([
    'list',
    (listInstances) => {
      listInstances.forEach((instance) => {
        instance.restart();
        document.querySelector(".use-case_cms-wrapper").style.visibility = "visible";
      });
    },
  ]);
</script>


<script>
  // 0) register all needed plugins up front
  gsap.registerPlugin(ScrollTrigger, DrawSVGPlugin);

  // Desktop animation: trigger once on the full list
  function createDesktopAnimation() {
    gsap.from(".integration-feature_icon path", {
      drawSVG: "0%",
      ease: "none",
      duration: 0.5,
      stagger: 0.3,
      scrollTrigger: {
        trigger: ".integration-feature_list",
        start: "50% bottom",
      },
    });
  }

  // Mobile animation: trigger per .integration-feature_item
  function createMobileAnimation() {
    document.querySelectorAll(".integration-feature_item").forEach((item) => {
      gsap.from(item.querySelectorAll(".integration-feature_icon path"), {
        drawSVG: "0%",
        ease: "none",
        duration: 0.5,
        stagger: 0.3,
        scrollTrigger: {
          trigger: item,
          start: "bottom bottom",
        },
      });
    });
  }

  function initAllAnimations() {
    let mm = gsap.matchMedia();

    // Define two conditions: desktop vs. mobile
    mm.add(
      {
        isDesktop: "(min-width: 768px)",
        isMobile: "(max-width: 767px)",
      },
      (context) => {
        let { isDesktop, isMobile } = context.conditions;

        if (isDesktop) {
          createDesktopAnimation();
        }
        if (isMobile) {
          createMobileAnimation();
        }
      }
    );
  }

  // Run after load + one frame so layouts are settled
  window.addEventListener("load", () => {
    requestAnimationFrame(initAllAnimations);
  });
</script>


<script>
document.querySelectorAll('.use-case_slider-component').forEach(component => {
  const sliderEl = component.querySelector('.use-case_slider');
  const prevBtn  = component.querySelector('.slider-arrow.is-prev');
  const nextBtn  = component.querySelector('.slider-arrow.is-next');

  new Swiper(sliderEl, {
    mousewheel:      { forceToAxis: true },
    speed:           800,
    keyboard:        true,
    slidesPerView:   'auto',
    spaceBetween:    8,
    navigation: {
      prevEl: prevBtn,
      nextEl: nextBtn,
      disabledClass: 'is-inactive'
    }
  });
});
</script></body></html>