<!-- Save referral code and promo code in cookies-->
<script>
  (function () {
    const urlParams = new URLSearchParams(window.location.search);
    const referral = urlParams.get('referral');
    const promoCode = urlParams.get('promo_code');
    
    if (referral) {
      const sanitizedReferral = encodeURIComponent(referral);
      const expiryDate = new Date();
      expiryDate.setTime(expiryDate.getTime() + 30 * 24 * 60 * 60 * 1000); // 30 days
      
      // Dynamic domain logic (matching your global code)
      const { hostname } = window.location;
      const domain = hostname.includes('wisprflow.ai') ? '.wisprflow.ai' : hostname;
      
      document.cookie = `referral=${sanitizedReferral};expires=${expiryDate.toUTCString()};path=/;domain=${domain};Secure;`;
    }
    
    if (promoCode) {
      const sanitizedPromoCode = encodeURIComponent(promoCode);
      const expiryDate = new Date();
      expiryDate.setTime(expiryDate.getTime() + 30 * 24 * 60 * 60 * 1000); // 30 days
      
      // Dynamic domain logic (matching your global code)
      const { hostname } = window.location;
      const domain = hostname.includes('wisprflow.ai') ? '.wisprflow.ai' : hostname;
      
      document.cookie = `promo_code=${sanitizedPromoCode};expires=${expiryDate.toUTCString()};path=/;domain=${domain};Secure;`;
    }
  })();
</script>


