<script>
document.addEventListener('DOMContentLoaded', function() {
    document.addEventListener('click', function(e) {
        const trigger = e.target.closest('[typeform-open]');
        
        if (trigger) {
            e.preventDefault();
            
            const typeformButton = document.querySelector('[data-tf-live] button');
            
            if (typeformButton) {
                typeformButton.click();
            } else {
                console.warn('Typeform button not found. Make sure the hidden div is loaded.');
            }
        }
    });
});
