<!--Nav dropdown flicker issue code-->
<script>
$('.nav_menu-dropdown-toggle-v2').on('mouseenter', function() {
 const hoveredDropdown = $(this);
 
 $('.dropdown-border-overlay').not(hoveredDropdown.find('.dropdown-border-overlay')).css('opacity', '0');
 
 const allDropdowns = $('.nav_menu-dropdown-toggle-v2');
 const currentIndex = allDropdowns.index(this);
 const nextDropdown = allDropdowns.eq(currentIndex + 1);
 
 if (nextDropdown.length) {
   const lastHoveredIndex = allDropdowns.data('lastHoveredIndex') || -1;
   const isSecondToFirst = lastHoveredIndex === 1 && currentIndex === 0;
   
   if (isSecondToFirst) {
     setTimeout(() => {
       nextDropdown.find('.dropdown1_border-radius-wrap').css('opacity', '0');
     }, 200);
   } else {
     nextDropdown.find('.dropdown1_border-radius-wrap').css('opacity', '0');
   }
 }
 
 allDropdowns.data('lastHoveredIndex', currentIndex);
}).on('mouseleave', function() {
 $('.dropdown-border-overlay').css('opacity', '');
 
 const allDropdowns = $('.nav_menu-dropdown-toggle-v2');
 const currentIndex = allDropdowns.index(this);
 const nextDropdown = allDropdowns.eq(currentIndex + 1);
 
 if (nextDropdown.length) {
   nextDropdown.find('.dropdown1_border-radius-wrap').css('opacity', '');
 }
});
</script>

<!--Close nav dropdown when you scroll down and dropdown is open-->
<script>
$(window).scroll(function() {
  const navbarStyle = $('.nav_fixed').attr('style');
  
  if (navbarStyle && navbarStyle.includes('translate3d(0px, -')) {
    
    if (window.matchMedia('(min-width: 992px)').matches) {
      $('.nav_menu-dropdown-toggle, .nav_menu-dropdown-toggle-v2').has('.w--open').find('.w--open').removeClass('w--open');
    } else {
      $('.nav_menu-dropdown-toggle, .nav_menu-dropdown-toggle-v2').has('.w--open').click();
      $('.nav_button.w--open').click();
    }
  }
});
</script>

<!--Button hover animation-->
<script>
function setupButtonAnimations(selector = ".button", targetSelector = null) {
  if (window.matchMedia("(hover: hover)").matches) {
    document.querySelectorAll(selector).forEach(element => {
      const animationTarget = targetSelector ? element.querySelector(targetSelector) : element;
      
      if (animationTarget) {
        const split = new SplitText(animationTarget, {
          type: "words,chars"
        });
        const chars = split.chars;
        let waveTimeline;
        
        element.addEventListener("mouseenter", () => {
          if (waveTimeline) waveTimeline.kill();
          waveTimeline = gsap.timeline();
          chars.forEach((char, i) => {
            waveTimeline.to(char, {
              keyframes: [{
                yPercent: -20,
                rotation: -5
              },
              {
                yPercent: 20,
                rotation: 5
              },
              {
                yPercent: 0,
                rotation: 0
              }],
              duration: 1,
              ease: "none",
              yoyo: true
            },
            i * 0.06);
          });
        });
        
        element.addEventListener("mouseleave", () => {
          if (waveTimeline) {
            waveTimeline.kill();
            gsap.to(chars, {
              yPercent: 0,
              rotation: 0,
              duration: 0.2,
              ease: "none"
            });
          }
        });
      }
    });
  }
}

// Usage:
setupButtonAnimations(); // Original buttons
setupButtonAnimations('.nav_menu-link'); // navbar links
setupButtonAnimations(".dropdown-link", ".dropdown-link-text"); // Dropdown links
</script>

<!-- 5) CTA dots moving on a path [Footer] -->
<script>
  function initCtaDots() {
    const ctaCount    = 100;
    const ctaDuration = 20;
    const pathOpts    = {
      path:        "#curve-cta",
      align:       "#curve-cta",
      alignOrigin: [0.5, 0.5],
    };
    
    const curveElement = document.querySelector("#curve-cta");
    if (!curveElement) {
    	return;
    }

    const svgWrapper = curveElement.closest("svg").parentNode;
    svgWrapper.style.position = "relative";

    for (let i = 0; i < ctaCount; i++) {
      const dot = document.createElement("div");
      dot.classList.add("cta_dot");
      gsap.set(dot, { position: "absolute" });
      svgWrapper.appendChild(dot);

      const tween = gsap.to(dot, {
        duration:        ctaDuration,
        repeat:          -1,
        ease:            "none",
        motionPath:      pathOpts,
        immediateRender: false,
      });
      tween.progress(i / ctaCount);
    }
  }

  // create a GSAP matchMedia instance
  const mm = gsap.matchMedia();

  // add a rule for screens ≥768px
  mm.add("(min-width: 768px)", () => {
    // when this rule first matches, wait for load + a frame then init
    window.addEventListener("load", () => {
      requestAnimationFrame(initCtaDots);
    });

    // return a cleanup function to remove the listener if rule no longer matches
    return () => {
      window.removeEventListener("load", initCtaDots);
    };
  });
</script>

<script>
// Find all elements with the attribute 'year'
const elements = document.querySelectorAll('[year]');
            
// Get the current year
const currentYear = new Date().getFullYear();

// Loop through all elements and set their text content to the current year
elements.forEach(element => {
    element.textContent = currentYear;
});
</script>

<!--Nextplay Adapta and Terra code-->
<script>
function setCookie(name, value, daysToExpire, domain) {
  const date = new Date();
  date.setTime(date.getTime() + daysToExpire * 24 * 60 * 60 * 1000);
  const expires = "expires=" + date.toUTCString();
  document.cookie = name + "=" + value + ";" + expires + ";path=/;domain=" + domain + ";secure;samesite=lax";
}

function getQueryParam(name) {
  const urlParams = new URLSearchParams(window.location.search);
  return urlParams.get(name);
}

if (getQueryParam('trial_promo') === '3mo') {
  setCookie('trial_promo', '3mo', 30, '.wisprflow.ai');
}
</script>

