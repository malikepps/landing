<script>
(function() {
  var params = new URLSearchParams(window.location.search);
  var tracking = new URLSearchParams();
  var utmParams = ['utm_source','utm_medium','utm_campaign','utm_content','utm_term'];

  tracking.set('project_id','eed39acc-d24c-4129-a771-db2bcea87cf8');

  for (var i = 0; i < utmParams.length; i++) {
    var value = params.get(utmParams[i]);
    if (value) tracking.set(utmParams[i], value);
  }

  if (document.referrer) tracking.set('page_referrer', document.referrer);

  var pixelUrl =
    'https://yqagkqabd5.us-east-2.awsapprunner.com/api/track/pixel.gif?' +
    tracking.toString();

  var img = new Image();
  img.src = pixelUrl;
  img.width = 1;
  img.height = 1;
  img.style.position = 'absolute';
  img.style.left = '-9999px';
  document.body.appendChild(img);

  !function(e, r) {
    try {
      if (e.vector) return;
      var t = {};
      t.q = t.q || [];
      var o = ["load", "identify", "on"];
      var n = function(name) {
        return function() {
          var args = Array.prototype.slice.call(arguments);
          t.q.push([name, args]);
        };
      };
      for (var c = 0; c < o.length; c++) {
        var a = o[c];
        t[a] = n(a);
      }
      if (!t.loaded) {
        var i = r.createElement("script");
        i.type = "text/javascript";
        i.async = true;
        i.src = "https://cdn.vector.co/pixel.js";
        var l = r.getElementsByTagName("script")[0];
        l.parentNode.insertBefore(i, l);
        t.loaded = true;
      }
      e.vector = t;
    } catch (err) {}
  }(window, document);

  window.vector.partnerId = 'eed39acc-d24c-4129-a771-db2bcea87cf8';
  vector.load("f9b6a15a-a9f8-4f25-8db6-dcfb8125f006");
})();
